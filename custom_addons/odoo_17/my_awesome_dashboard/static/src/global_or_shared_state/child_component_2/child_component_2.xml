<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
    <t t-name="my_awesome_dashboard.ChildComponent2">
        <div class="card mb-3">
    <div class="card-header">
        <h5 class="card-title mb-0">Counter</h5>
    </div>
    <div class="card-body text-center">
        <div class="display-4" t-esc="count_obj.count.value"></div>
        <div class="mt-3">
            <button class="btn btn-primary mx-2" t-on-click="increment">+</button>
            <button class="btn btn-danger mx-2" t-on-click="decrement">-</button>
        </div>
    </div>
</div>
    </t>
</templates>